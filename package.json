{
  "license": "MIT",
  "name": "@vesp/frontend",
  "description": "Nuxt 3 based frontend for your Vesp project",
  "version": "5.0.0-rc.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./assets/components": "./assets/_components.scss",
    "./assets/toast": "./assets/_toast.scss"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist",
    "assets"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezumkin/vesp-frontend.git"
  },
  "author": "bezumkin",
  "bugs": {
    "url": "https://github.com/bezumkin/vesp-frontend/issues"
  },
  "homepage": "https://github.com/bezumkin/vesp-frontend#readme",
  "scripts": {
    "analyze": "nuxi analyze",
    "build": "nuxt-module-build build",
    "prepare": "nuxt-module-build prepare && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "typecheck": "nuxt typecheck",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "stylelint": "stylelint ./assets/**/*.scss",
    "stylelint:fix": "stylelint --fix --quiet ./assets/**/*.scss",
    "release": "npm run lint && npm run stylelint && npm run prepare && npm run build && npm publish"
  },
  "dependencies": {
    "@bootstrap-vue-next/nuxt": "^0.30.4",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@nuxtjs/i18n": "^10.0.2",
    "@pinia/nuxt": "^0.11.2",
    "@vesp/nuxt-fontawesome": "^2.0.0-rc.2",
    "bootstrap-scss": "^5.3.4",
    "bootstrap-vue-next": "^0.30.4",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.3",
    "slugify": "^1.6.6",
    "text-mask-core": "^5.1.2",
    "vue-datepicker-next": "^1.0.3",
    "vue-i18n": "^11.1.11",
    "vue-toastification": "2.0.0-rc.5"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.7.4",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.0.1",
    "@nuxt/cli": "^3.26.4",
    "@nuxt/kit": "^4.0.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nuxt": "^4.0.1",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.4.2",
    "rollup": "^4.30.0",
    "sass": "~1.79.6",
    "sass-loader": "^16.0.4",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-plugin-stylelint": "^6.0.0",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
  }
}
